
.footer-h {
	height: 100rpx;
	height: calc(100rpx + constant(safe-area-inset-bottom));
	height: calc(100rpx + env(safe-area-inset-bottom));
}
.footer-box {
	width: 100%;
	position: fixed;
	z-index: 100;
	left: 0;
	bottom: 0;
	background: #FFFFFF;
	box-shadow: 0rpx -4rpx 16rpx 0rpx rgba(0, 0, 0, 0.04);
}
.footer-main {
	height: 100rpx;
}
.footer-item {
	height: 100rpx;
	width: 20%;
	padding-top: 10rpx;
	text-align: center;
}

